{
  "name": "@icebreakers/stylelint-config",
  "type": "module",
  "version": "1.1.3",
  "description": "icebreaker's stylelint config",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/eslint-config.git",
    "directory": "packages/stylelint"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/eslint-config/issues"
  },
  "keywords": [
    "stylelint"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": "./dev/bin.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "sync": "cnpm sync @icebreakers/stylelint-config"
  },
  "dependencies": {
    "comment-json": "^4.2.5",
    "postcss-html": "^1.8.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-recommended-scss": "^16.0.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "bin": "./bin/init.js",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  }
}
