{
  "name": "@icebreakers/commitlint-config",
  "type": "module",
  "version": "1.2.2",
  "description": "Commitlint preset from Icebreaker's dev-configs",
  "author": "ice breaker <1324318532@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/dev-configs.git",
    "directory": "packages/commitlint"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/dev-configs/issues"
  },
  "keywords": [
    "commitlint",
    "dev-configs",
    "icebreakers"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build:watch": "tsup --watch --sourcemap",
    "build": "tsup",
    "test": "vitest run",
    "test:dev": "vitest",
    "release": "pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "publishConfig": {},
  "dependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0"
  }
}
