{
  "name": "icebreaker-eslint-config",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.14.0",
  "author": "ice breaker <1324318532@qq.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/eslint-config/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "turbo run lint",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "postinstall": "pnpm run build",
    "commit": "commit",
    "sync": "turbo run sync",
    "inspector": "npx @eslint/config-inspector",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "script:init": "monorepo init",
    "script:sync": "monorepo sync",
    "script:clean": "monorepo clean",
    "script:mirror": "monorepo mirror",
    "-------": "-----------",
    "release": "changeset",
    "pr": "changeset pre enter alpha",
    "pr:beta": "changeset pre enter beta",
    "pr:rc": "changeset pre enter rc",
    "pr:exit": "changeset pre exit",
    "cv": "changeset version ",
    "--------": "----------------",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint/config-inspector": "^1.1.0",
    "@eslint/eslintrc": "^3.3.1",
    "@icebreakers/commitlint-config": "workspace:*",
    "@icebreakers/eslint-config": "workspace:*",
    "@icebreakers/monorepo": "^1.0.10",
    "@icebreakers/stylelint-config": "workspace:*",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.2.1",
    "@types/set-value": "^4.0.3",
    "@vitest/coverage-v8": "~3.2.4",
    "ci-info": "^4.3.0",
    "cross-env": "^10.0.0",
    "defu": "^6.1.4",
    "es-toolkit": "^1.39.9",
    "eslint": "^9.33.0",
    "eslint-flat-config-utils": "^2.1.1",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "get-value": "^4.0.1",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.8",
    "only-allow": "^1.2.1",
    "pathe": "^2.0.3",
    "pkg-types": "^2.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "set-value": "^4.1.0",
    "stylelint": "^16.23.1",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.0",
    "vitest": "~3.2.4",
    "yaml": "^2.8.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
