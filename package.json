{
  "name": "eslint-config",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "pnpm -r publish --access public",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@icebreakers/eslint-config-basic": "workspace:*",
    "@icebreakers/eslint-config-ts": "workspace:*",
    "@types/eslint": "^8.21.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "eslint": "^8.35.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "turbo": "^1.8.3",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.1"
}