{
  "name": "pnpm-turbo-monorepo-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/eslint": "^8.4.9",
    "eslint": "^8.23.0",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.8.2",
    "@icebreakers/eslint-config-basic": "workspace:*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.1"
}